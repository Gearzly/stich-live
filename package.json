{
  "name": "stich-production",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered web application generator built with Vercel and Firebase",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "npm run build && vercel",
    "functions:build": "cd functions && npm run build",
    "functions:deploy": "firebase deploy --only functions",
    "functions:serve": "cd functions && npm run serve",
    "firestore:deploy": "firebase deploy --only firestore",
    "storage:deploy": "firebase deploy --only storage",
    "firebase:deploy": "firebase deploy",
    "firebase:emulators": "firebase emulators:start",
    "setup": "node scripts/setup.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "firebase": "^10.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "sonner": "^1.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "firebase-tools": "^13.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^6.0.1",
    "vitest": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/user-event": "^14.5.0"
  },
  "keywords": [
    "ai",
    "code-generation", 
    "web-development",
    "react",
    "firebase",
    "vercel",
    "typescript"
  ],
  "author": "Stich Production Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gearzly/stich-production.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}