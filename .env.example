# Stich Production Environment Variables

# Application Info
VITE_APP_NAME="Stich Production"
VITE_APP_VERSION="1.0.0"
VITE_APP_URL="http://localhost:3000"

# Firebase Configuration
VITE_FIREBASE_API_KEY="your-firebase-api-key"
VITE_FIREBASE_AUTH_DOMAIN="your-project.firebaseapp.com"
VITE_FIREBASE_PROJECT_ID="your-project-id"
VITE_FIREBASE_STORAGE_BUCKET="your-project.appspot.com"
VITE_FIREBASE_MESSAGING_SENDER_ID="123456789"
VITE_FIREBASE_APP_ID="your-app-id"

# API URLs
VITE_API_BASE_URL="http://localhost:3001"
VITE_FUNCTIONS_BASE_URL="http://localhost:5001/your-project-id/us-central1"

# AI Provider API Keys (Optional - can be configured per user)
VITE_OPENAI_API_KEY=""
VITE_ANTHROPIC_API_KEY=""
VITE_GOOGLE_AI_API_KEY=""
VITE_CEREBRAS_API_KEY=""

# WebSocket Configuration
VITE_WS_URL="ws://localhost:3001"

# Feature Flags
VITE_FEATURE_AUTH="true"
VITE_FEATURE_REALTIME="true"
VITE_FEATURE_EXECUTION="true"
VITE_FEATURE_DEPLOYMENT="true"
VITE_FEATURE_ANALYTICS="true"
VITE_FEATURE_PAYMENTS="false"
VITE_FEATURE_ADMIN="true"

# Development
VITE_DEBUG="false"
VITE_LOG_LEVEL="info"

# Analytics (Optional)
VITE_ANALYTICS_ID=""
VITE_SENTRY_DSN=""

# Production URLs (for Vercel deployment)
# VITE_APP_URL="https://your-app.vercel.app"
# VITE_API_BASE_URL="https://your-api.vercel.app"
# VITE_WS_URL="wss://your-api.vercel.app"
